{
    "name": "sander-van-hooft/laravel-payable-redirect-mollie",
    "type": "library",
    "description": "Associate Mollie payments with Laravel Eloquent models",
    "keywords": [
        "eloquent",
        "laravel",
        "mollie",
        "payable",
        "redirect",
        "sandervanhooft"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sander van Hooft",
            "email": "info@sandervanhooft.nl",
            "homepage": "http://www.sandervanhooft.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/database": "^5.4",
        "illuminate/support": "^5.4",
        "guzzlehttp/guzzle": "^6.2",
        "mollie/mollie-api-php": "1.9.*",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "mockery/mockery": "^0.9.9",
        "orchestra/database": "^3.4",
        "phpunit/phpunit": "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "SanderVanHooft\\PayableRedirect\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SanderVanHooft\\PayableRedirect\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
